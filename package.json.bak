{
  "name": "billbuzz",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "electron/main.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "next dev -p 3000",
    "start": "next start -p 3000",
    "build": "next build",
    "lint": "next lint",
    "electron:dev": "cross-env ELECTRON_DEV=true NODE_ENV=development electron .",
    "electron:prod": "cross-env NODE_ENV=production electron .",
    "dist": "npm run build && npm run build:server:prod && electron-builder --dir",
    "package": "npm run build && npm run build:server:prod && electron-builder",
    "build:server:prod": "cd server && npm ci --omit=dev --production && cd .."
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@vercel/analytics": "1.3.1",
    "bcryptjs": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "cors": "latest",
    "date-fns": "4.1.0",
    "dotenv": "^17.2.3",
    "embla-carousel-react": "8.5.1",
    "express": "latest",
    "geist": "^1.3.1",
    "input-otp": "1.4.1",
    "jsbarcode": "latest",
    "jsonwebtoken": "latest",
    "jspdf": "latest",
    "lucide-react": "^0.454.0",
    "mongoose": "latest",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-day-picker": "9.8.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.60.0",
    "react-resizable-panels": "^2.1.7",
    "react-router-dom": "^7.9.5",
    "recharts": "2.15.4",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.9",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.17",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "10.4.21",
    "cross-env": "^7.0.3",
    "electron": "39.1.1",
    "electron-builder": "^26.0.12",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.17",
    "tw-animate-css": "1.3.3",
    "typescript": "^5"
  },
  "build": {
    "asar": false,
    "appId": "com.yourcompany.myapp",
    "productName": "BillBuzz",
    "files": [
      "**/*",
      "!dist/**",
      "!**/*.map",
      "!**/*.log"
    ],
    "extraResources": [
      {
        "from": "server/",
        "to": "server/",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": ".next/standalone",
        "to": ".next/standalone",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": ".next/static",
        "to": ".next/static",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "assets"
    }
  }
}